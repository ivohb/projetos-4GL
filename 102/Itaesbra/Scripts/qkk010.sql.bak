
create table qkk010 
  (
    qkk_filial varchar(2) 
        default '' not null ,
    qkk_peca varchar(40) 
        default '' not null ,
    qkk_rev varchar(2) 
        default '' not null ,
    qkk_revinv varchar(2) 
        default '',
    qkk_item varchar(4) 
        default '',
    qkk_nope varchar(6) 
        default '' not null ,
    qkk_desc varchar(50) 
        default '',
    qkk_maq varchar(50) 
        default '',
    qkk_func varchar(50) 
        default '',
    qkk_area varchar(20) 
        default '',
    qkk_tpope varchar(1) 
        default '',
    qkk_sbope varchar(2) 
        default '',
    qkk_chave varchar(8) 
        default '',
    r_e_c_d_e_l_ float 
        default 0.0000000000000000 not null ,
    d_e_l_e_t_ varchar(1) 
        default '',
    r_e_c_n_o_ integer 
        default 0 not null ,
    primary key (r_e_c_n_o_) 
  );

create index qkk0101 on qkk010 (qkk_filial,qkk_peca,
    qkk_rev,qkk_item,r_e_c_n_o_,d_e_l_e_t_);
    
create index qkk0102 on qkk010 (qkk_filial,qkk_peca,
    qkk_rev,qkk_nope,r_e_c_n_o_,d_e_l_e_t_);
    
create index qkk0103 on qkk010 (qkk_filial,qkk_peca,
    qkk_revinv,qkk_item,r_e_c_n_o_,d_e_l_e_t_);
    
create index qkk0104 on qkk010 (qkk_filial,qkk_chave,
    r_e_c_n_o_,d_e_l_e_t_);
    
create unique index qkk010_unq on qkk010 (qkk_filial,
    qkk_peca,qkk_rev,qkk_nope,r_e_c_d_e_l_);


