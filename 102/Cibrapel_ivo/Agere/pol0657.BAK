DATABASE logix
SCREEN
{
-----------------------------------------------------------------
[a0]         CENTROS DE CUSTOS NAO TRANSFERIDOS    
 
 Empresa Origem  :[a1][a2                                  ]

 Empresa Destino :[a3][a4                                  ] 

 Centro de custo :[a5        ][a6                          ]

}   
END
TABLES
cc_n_trans
END
ATTRIBUTES
a0 = FORMONLY.cod_empresa, UPSHIFT, NOENTRY;
