 e = FORMONLY.dat_fim TYPE DATE, FORMAT = 'dd/mm/yyyy', AUTONEXT,
     COMMENTS = "Digite a Data Final";
 f = FORMONLY.dat_fec TYPE DATE, FORMAT = 'dd/mm/yyyy', NOENTRY, REVERSE, color = red;
