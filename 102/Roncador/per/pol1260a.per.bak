  d = FORMONLY.dat_entrada_nf, AUTONEXT;
  e = FORMONLY.cod_fornecedor
      --# ,WIDGET='FIELD_BMP', CONFIG='combo.bmp Control-z';
  f = FORMONLY.raz_social, NOENTRY;  
  g = FORMONLY.num_aviso_rec;
  h = FORMONLY.num_nf;
  i = FORMONLY.ser_nf;
  j = FORMONLY.ssr_nf;
  k = FORMONLY.ies_especie_nf;

END 

