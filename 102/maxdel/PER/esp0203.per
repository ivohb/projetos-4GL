a3   = desc_nat_oper.cod_nat_oper, UPSHIFT, AUTONEXT;    
a4   = nat_operacao.den_nat_oper, NOENTRY; 
a5   = desc_nat_oper.pct_desc_valor, UPSHIFT, AUTONEXT;    
a6   = desc_nat_oper.pct_desc_qtd, UPSHIFT, AUTONEXT;    
a7   = desc_nat_oper.pct_desc_oper, UPSHIFT, AUTONEXT;    

END
