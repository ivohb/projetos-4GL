DATABASE logix
SCREEN
{
------------------------------------------------------------------------------
[a ]                   USUARIO NÍVEL SUBSTITUTO

  
       Usuário titular:[b       ]  [c                                  ]
            
         Número versão:[d ]              Versão atual:[e]

    Usuário substituto:[f       ]  [g                                  ]              

   Período de validade:[h         ] - [i         ] 

Motivo da substituição:[j                                                 ]

            
         ------------- P R O C E S S A M E N T O -------------
      
         Usuário:[o       ]  Data:[p         ]  Hora:[q       ]
}

ATTRIBUTES
 a = FORMONLY.cod_empresa,       UPSHIFT, NOENTRY;
 b = FORMONLY.cod_usuario,       AUTONEXT
     --# ,WIDGET='FIELD_BMP', CONFIG='combo.bmp Control-z';
