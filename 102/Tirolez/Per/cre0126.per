e = tirolez_clientes.cod_zona,         UPSHIFT, AUTONEXT;
f = tirolez_clientes.cod_roteiro,      UPSHIFT, AUTONEXT;     
g = tirolez_clientes.cod_empresa,      UPSHIFT, AUTONEXT
--#, WIDGET="FIELD_BMP", CONFIG="combo.bmp Control-z"
;
g1= FORMONLY.den_empresa,         		 NOENTRY;
h = tirolez_clientes.desc_finan,       UPSHIFT, AUTONEXT;      
i = tirolez_clientes.ies_tip_desc,     UPSHIFT, AUTONEXT; 
j = tirolez_clientes.cod_seguimento,   UPSHIFT, AUTONEXT
--#, WIDGET="FIELD_BMP", CONFIG="combo.bmp Control-z"
;
j1= FORMONLY.den_seguimento,					 NOENTRY;
