 f = FORMONLY.cod_usuario_subs,  AUTONEXT
     --# ,WIDGET='FIELD_BMP', CONFIG='combo.bmp Control-z'; 
 g = FORMONLY.nom_usuario_subs,  UPSHIFT, NOENTRY;
 h = FORMONLY.dat_ini_validade,  UPSHIFT, AUTONEXT;
 i = FORMONLY.dat_fim_validade,  UPSHIFT, AUTONEXT;
 j = FORMONLY.motivo_subs,       UPSHIFT, AUTONEXT;
 o = FORMONLY.cod_usuario_incl,  NOENTRY;
 p = FORMONLY.dat_inclusao,      NOENTRY;
 q = FORMONLY.hor_inclusao,      NOENTRY;
