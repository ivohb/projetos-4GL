DATABASE FORMONLY
SCREEN
{
 -----------------------------------------------------------------------------
  [a ]                         RELAÇÃO MENSAL DESCONTOS

          Banco          : [c  ] 
          Classificar Por: [b] 
          UF             : [d ]
          Setor          : [g ]
          Mês            : [e1] 
          Ano            : [f234] 
}
END


ATTRIBUTES
a = FORMONLY.cod_empresa, NOENTRY;
c = FORMONLY.cod_banco, UPSHIFT, AUTONEXT
--# ,WIDGET='FIELD_BMP', CONFIG='combo.bmp Control-z';
b = FORMONLY.cod_class, UPSHIFT, AUTONEXT,
    COMMENTS = '1-Nome  2-CPF';
d = FORMONLY.cod_uf, UPSHIFT, AUTONEXT,
    COMMENTS = '99-Todos'
--# ,WIDGET='FIELD_BMP', CONFIG='combo.bmp Control-z';
g = FORMONLY.cod_setor, UPSHIFT, AUTONEXT,
    COMMENTS = '99-Todos'
--# ,WIDGET='FIELD_BMP', CONFIG='combo.bmp Control-z';
e1 = FORMONLY.cod_tipo, UPSHIFT, AUTONEXT,
f234 = FORMONLY.cod_posicao, UPSHIFT, AUTONEXT,
END 
