     COMMENTS = "Digite o Codigo da Empresa Origem";
a2 = FORMONLY.den_empresa_orig TYPE CHAR, UPSHIFT, NOENTRY;
a3 = cc_n_trans.cod_empresa_dest, UPSHIFT, AUTONEXT,
     COMMENTS = "Digite o Codigo da Empresa Destino";
a4 = FORMONLY.den_empresa_dest TYPE CHAR, UPSHIFT, NOENTRY;     
a5 = cc_n_trans.cod_cent_cust, AUTONEXT,
     COMMENTS = "Digite o Codigo do Centro de Custo";
a6 = FORMONLY.den_cent_cust TYPE CHAR, UPSHIFT, NOENTRY;     
END
