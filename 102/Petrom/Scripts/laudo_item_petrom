
create table laudo_item_petrom 
  (
    cod_empresa char(2) not null,
    num_laudo decimal(6,0) not null,
    tip_analise decimal(6,0) not null,
    metodo char(20),
    especificacao_de decimal(10,4),
    especificacao_ate decimal(10,4),
    resultado decimal(10,4),
    tipo_valor char(2),
    analise_bloq char(1)
  );

create unique index ix_laudo_item_1 on laudo_item_petrom 
    (cod_empresa,num_laudo,tip_analise);