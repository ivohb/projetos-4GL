 m = FORMONLY.num_nf,      NOENTRY  --# ,OPTIONS='-nolist';
 q = FORMONLY.dat_entrada, NOENTRY  --# ,OPTIONS='-nolist';
 s = FORMONLY.num_ar,      NOENTRY  --# ,OPTIONS='-nolist';
 u = FORMONLY.cod_status,  NOENTRY  --# ,OPTIONS='-nolist';
END 

INSTRUCTIONS
   SCREEN RECORD sr_nf[11] (cod_for THRU cod_status);
