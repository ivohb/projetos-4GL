DATABASE FORMONLY 
SCREEN
{
--------------------------------------------------------------------------------
[a ]              FRETE SAÍDA - RELACIONAMENTO ENTRE NOTAS         Status:[z]

N NF:[b        ] Fornec.:[c              ] [d                 ] Emis:[e       ]
  Ser:[f ]   Ssr:[g ]   Esp:[h  ]    V Frete:[i         ]    ICMS:[j        ]  
  
      Num NF    Peso        Cliente            Destino
  [m         |q          |r              |s                               ]
  [m         |q          |r              |s                               ]
  [m         |q          |r              |s                               ]
  [m         |q          |r              |s                               ]
  [m         |q          |r              |s                               ]
  [m         |q          |r              |s                               ]
  [m         |q          |r              |s                               ]
  [m         |q          |r              |s                               ]
  [m         |q          |r              |s                               ]
  [m         |q          |r              |s                               ]

}
END 

ATTRIBUTES
 a = FORMONLY.cod_empresa,    NOENTRY;
 z = FORMONLY.ies_validado,   NOENTRY, REVERSE, color = red;
 b = FORMONLY.num_nfe,        AUTONEXT;
 c = FORMONLY.cod_for,        AUTONEXT
     --# ,WIDGET='FIELD_BMP', CONFIG='combo.bmp Control-z',OPTIONS='-nolist';
 d = FORMONLY.nom_for,        NOENTRY, REVERSE;
 e = FORMONLY.dat_nfe,        NOENTRY, REVERSE, FORMAT = 'dd/mm/yy';
 f = FORMONLY.ser_nfe,        NOENTRY, REVERSE;
 g = FORMONLY.ssr_nfe,        NOENTRY, REVERSE;
 h = FORMONLY.esp_nfe,        NOENTRY, REVERSE;
 i = FORMONLY.val_nfe,        NOENTRY, REVERSE;
 j = FORMONLY.val_icm,        NOENTRY, REVERSE;
 m = FORMONLY.num_nff,        AUTONEXT;
 q = FORMONLY.pes_tot_bruto,  NOENTRY, REVERSE;
 r = FORMONLY.nom_reduzido,   NOENTRY, REVERSE;
 s = FORMONLY.den_cidade,     NOENTRY, REVERSE;
END

INSTRUCTIONS
   SCREEN RECORD sr_nota[10] (num_nff THRU den_cidade);
END
