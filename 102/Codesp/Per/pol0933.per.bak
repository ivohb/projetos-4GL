DATABASE logix

SCREEN
{
------------------------------------------------------------------------------
 [a0]    CADASTRO DE PARAMETROS P/ IMPORTAÇÃO DE SOLICITAÇAO DE FATURA
        
        Cód.Parametro: [a         ] 
        Den.Parametro: [a1                                ]
        
       Cond.Pagamento: [b  ]  [b1                            ]
 Classificação Fiscal: [c         ] 
           Finalidade: [d]
       Item Produzido: [e] 
                Moeda: [f ]   [f1             ]
         Nat.Operacao: [g  ]  [g1                            ]
   Nat.Oper.Não Trib.: [n  ]  [n1                            ]
        Tipo carteira: [h  ]  [h1             ]
           Tipo preço: [i]
           Tipo Venda: [j ]   [j1             ]
   Cam.Exrpot.Arquivo: [m                                                ]
        
	  
}         


TABLES
   par_solc_fat_codesp
END

ATTRIBUTES
a0 =  FORMONLY.cod_empresa,      									UPSHIFT,  NOENTRY;
a =  par_solc_fat_codesp.cod_parametro, 					UPSHIFT,AUTONEXT;
a1 = par_solc_fat_codesp.den_parametro, 					UPSHIFT,AUTONEXT;
b	=  par_solc_fat_codesp.COND_PAGTO,							UPSHIFT,AUTONEXT
--#,WIDGET="FIELD_BMP", CONFIG="combo.bmp Control-z";
b1	=  FORMONLY.den_COND_PAGTO, 									UPSHIFT,  NOENTRY;
c	=  par_solc_fat_codesp.clas_fiscal, 						UPSHIFT,AUTONEXT
--#,WIDGET="FIELD_BMP", CONFIG="combo.bmp Control-z";
d	=  par_solc_fat_codesp.finalidade, 						 	UPSHIFT,AUTONEXT,
		 COMMENTS = "1-contribuinte 2-nao contribuinte 3-contribuinte uso";
		 

e	=  par_solc_fat_codesp.item_prod,								UPSHIFT,AUTONEXT,
		 COMMENTS = "'C-Comprado','B-Beneficiado','F-Final' e 'P-Produzido'";
f	=  par_solc_fat_codesp.moeda,										UPSHIFT,AUTONEXT
--#,WIDGET="FIELD_BMP", CONFIG="combo.bmp Control-z";
f1	=  FORMONLY.den_moeda,												UPSHIFT,  NOENTRY;
g	=  par_solc_fat_codesp.natureza_operacao,				UPSHIFT,AUTONEXT
--#,WIDGET="FIELD_BMP", CONFIG="combo.bmp Control-z";
g1=  FORMONLY.den_natureza_operacao,							UPSHIFT,  NOENTRY;
n	=  par_solc_fat_codesp.nat_oper_nao_trib,				UPSHIFT,AUTONEXT
--#,WIDGET="FIELD_BMP", CONFIG="combo.bmp Control-z";
n1	=  FORMONLY.den_nat_oper_nao_trib,						UPSHIFT,  NOENTRY;
h	=  par_solc_fat_codesp.TIP_CARTEIRA,						UPSHIFT,AUTONEXT
--#,WIDGET="FIELD_BMP", CONFIG="combo.bmp Control-z";
h1=  FORMONLY.den_TIP_CARTEIRA, 									UPSHIFT,  NOENTRY;
i	=  par_solc_fat_codesp.tipo_preco,							UPSHIFT,AUTONEXT,
		 COMMENTS = "'F-Firme','R-Reajustavel'";
j	=  par_solc_fat_codesp.tipo_venda,							UPSHIFT,AUTONEXT
--#,WIDGET="FIELD_BMP", CONFIG="combo.bmp Control-z";
j1	=  FORMONLY.den_tipo_venda, 									UPSHIFT,  NOENTRY;
m = par_solc_fat_codesp.cam_export,								UPSHIFT,AUTONEXT;

END 	
