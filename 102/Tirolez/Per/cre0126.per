DATABASE logix

SCREEN
{
--------------------------------------------------------------------------------
                      COMPLEMENTO CLIENTE (TIROLEZ_CLIENTES)

        Cliente:[a              ] [b                                       ]
            EAN:[c             ]
          Setor:[d ]
           Zona:[e    ]
        Roteiro:[f  ]
        Empresa:[g ] [g1                                 ]
     % Desconto:[h       ]
  Tipo desconto:[i]
     Seguimento:[j] [j1                            ]
  Tipo perueiro:[l] [l1                            ]    
           EAN1:[m             ]   Aceita Um Terço: [n]      Preço Flag: [o]
Cód canal venda:[p  ]  [q                                      ]
  Cód atividade:[r  ]  [s                                      ]
Cód atendimento:[ca]  [da                                      ]
    Validar Peso no Coletor:[t]        Pct peso acima do solicitado:[u ]
     Faixa de produto utilizada para validar os produtos no coletor:[v ]
  Cliente utiliza EDI (S ou N):[x]  Integração Automática Logística:[z] S/N
}
END

TABLES
tirolez_clientes
END

ATTRIBUTES
a = tirolez_clientes.cod_cliente,      UPSHIFT     
