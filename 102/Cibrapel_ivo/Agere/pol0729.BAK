ATTRIBUTES
 a = FORMONLY.cod_empresa, NOENTRY;
 b = FORMONLY.cod_cliente, UPSHIFT, AUTONEXT
     --# ,WIDGET='FIELD_BMP', CONFIG='combo.bmp Control-z';
 c = FORMONLY.nom_cliente, UPSHIFT, NOENTRY;
 d = FORMONLY.dat_ini TYPE DATE, FORMAT = 'dd/mm/yyyy', AUTONEXT,
     COMMENTS = "Digite a Data Inicial";
 e = FORMONLY.dat_fim TYPE DATE, FORMAT = 'dd/mm/yyyy', AUTONEXT,
     COMMENTS = "Digite a Data Final";

 g = FORMONLY.nf_dev,   NOENTRY --#, OPTIONS="-nolist";
 h = FORMONLY.cod_item, NOENTRY --#, OPTIONS="-nolist";
 i = FORMONLY.qtd_dev,  NOENTRY --#, OPTIONS="-nolist";
 j = FORMONLY.val_dev,  NOENTRY --#, OPTIONS="-nolist";
 k = FORMONLY.nf_orig,  NOENTRY --#, OPTIONS="-nolist";
 l = FORMONLY.pct_desc, NOENTRY --#, OPTIONS="-nolist";
 m = FORMONLY.tip_desc, NOENTRY --#, OPTIONS="-nolist";
 n = FORMONLY.qtd_lanc, NOENTRY --#, OPTIONS="-nolist";
 o = FORMONLY.val_lanc, NOENTRY --#, OPTIONS="-nolist";
 p = FORMONLY.efetivar, AUTONEXT,
     INCLUDE=('S','N'), COMMENTS='S-Marcado N-Não Marcado'
     --#, widget='CHECK', config='S N { }', OPTIONS="-nolist";  
END 

INSTRUCTIONS
   SCREEN RECORD sr_notas[10](nf_dev THRU efetivar);
END

